#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alinnechita/projects/cocktail/node_modules/.pnpm/isomorphic-git@0.70.0/node_modules/isomorphic-git/node_modules:/Users/alinnechita/projects/cocktail/node_modules/.pnpm/isomorphic-git@0.70.0/node_modules:/Users/alinnechita/projects/cocktail/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alinnechita/projects/cocktail/node_modules/.pnpm/isomorphic-git@0.70.0/node_modules/isomorphic-git/node_modules:/Users/alinnechita/projects/cocktail/node_modules/.pnpm/isomorphic-git@0.70.0/node_modules:/Users/alinnechita/projects/cocktail/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
